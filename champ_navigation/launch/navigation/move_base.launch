<launch>
    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
        <rosparam file="$(find champ_navigation)/param/costmap_common_params.yaml" command="load" ns="global_costmap" />
        <rosparam file="$(find champ_navigation)/param/costmap_common_params.yaml" command="load" ns="local_costmap" />
        <rosparam file="$(find champ_navigation)/param/local_costmap_params.yaml" command="load" />
        <rosparam file="$(find champ_navigation)/param/global_costmap_params.yaml" command="load" />
        <rosparam file="$(find champ_navigation)/param/base_local_planner_holonomic_params.yaml" command="load" />
        <rosparam file="$(find champ_navigation)/param/move_base_params.yaml" command="load" />
    </node>
</launch>
